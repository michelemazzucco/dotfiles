[user]
  name = Michele Mazzucco
  email = hello@michelemazzucco.it
[push]
  default = current
[github]
  user = michelemazzucco
[alias]
  s = status
  status = status
  a = add -A .
  co = checkout
  cm = commit
  p = push
[merge]
  tool = opendiff
[credential]
  helper = osxkeychain
[core]
  excludesfile = ~/.gitignore
	editor = vim
[color]
  ui = auto
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
